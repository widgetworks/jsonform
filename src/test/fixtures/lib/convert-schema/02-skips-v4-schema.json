{
    "$description": "V4 schemas should be returned unmodified",
    "$source": {
        "title": "CustomerV4",
        "description": "json-schema v4 style 'required'",
        "type": "object",
        "required": [
            "name",
            "address",
            "phoneNumber"
        ],
        "properties": {
            "name": {
                "title": "Name",
                "type": "string"
            },
            "address": {
                "title": "Address",
                "type": "object",
                "required": [
                    "steet",
                    "city"
                ],
                "properties": {
                    "city": {
                        "title": "City",
                        "type": "string"
                    },
                    "street": {
                        "title": "Street",
                        "type": "string"
                    },
                    "zip": {
                        "title": "Zip",
                        "type": "string"
                    }
                }
            },
            "phoneNumber": {
                "type": "array",
